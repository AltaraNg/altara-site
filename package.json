{
    "name": "altara-site",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
        "prepare": "husky install",
        "postinstall": "husky install"
    },
    "dependencies": {
        "@j-t-mcc/vue3-chartjs": "^1.3.0",
        "@vueuse/core": "^9.1.0",
        "aws-s3-upload-ash": "^1.1.4",
        "aws-sdk": "^2.1204.0",
        "axios": "^0.25.0",
        "chart.js": "^3.7.0",
        "core-js": "^3.6.5",
        "vue": "^3.0.0",
        "vue-axios": "^3.4.0",
        "vue-currency-input": "^3.0.0",
        "vue-router": "^4.0.12",
        "vue3-animate-onscroll": "^1.0.6",
        "vue3-autocounter": "^1.0.6",
        "vue3-carousel": "^0.1.35",
        "vue3-slider": "^1.8.0",
        "vue3-smooth-scroll": "^0.8.1"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.22.15",
        "@tailwindcss/postcss7-compat": "^2.2.17",
        "@vue/cli-plugin-babel": "^5.0.8",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0",
        "autoprefixer": "^9.8.8",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-vue": "^9.17.0",
        "husky": "^8.0.0",
        "lint-staged": "^14.0.1",
        "postcss": "^7.0.39",
        "prettier": "^3.0.3",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "lint-staged": {
        "*.{vue,js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ]
    }
}
